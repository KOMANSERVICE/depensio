{
  "ConnectionStrings": {
    "DataBase": "DataBaseLocal"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Vault": {
    "PathMountPoint": "depensio",
    "MountPoint": "secret"
  },
  "MailConfig": {
    "FromMailId": "MAIL_CONFIG_ID",
    "FromMailIdPassword": "FromMailIdPassword",
    "FromMailName": "MAIL_CONFIG_NAME",
    "Host": "MAIL_CONFIG_HOST",
    "Ports": "MAIL_CONFIG_PORT"
  },
  "JWT": {
    "ValidAudience": "JWT_VALID_AUDIENCE",
    "ValidIssuer": "JWT_VALID_ISSUER",
    "Secret": "Secret"
  },
  "Allow": {
    "Origins": "CorsAllowedOrigins"
  }
}
