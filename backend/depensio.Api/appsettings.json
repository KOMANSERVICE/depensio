{
  "ConnectionStrings": {
    "DataBase": "CONNECTION_STRING_API"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Vault": {
    "Uri": "VAULT_ADDR",
    "RoleId": "VAULT_ROLE_ID",
    "SecretId": "VAULT_SECRET_ID",
    "Path": "VAULT_PATH"
  },
  "MailConfig": {
    "FromMailId": "MAIL_CONFIG_ID",
    "FromMailIdPassword": "MAIL_CONFIG_PASSWORD",
    "FromMailName": "MAIL_CONFIG_NAME",
    "Host": "MAIL_CONFIG_HOST",
    "Ports": "MAIL_CONFIG_PORT"
  },
  "JWT": {
    "ValidAudience": "JWT_VALID_AUDIENCE",
    "ValidIssuer": "JWT_VALID_ISSUER",
    "Secret": "JWT_SECRET"
  }
}
