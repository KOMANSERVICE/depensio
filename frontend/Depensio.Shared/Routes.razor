
<AuthInitializer PInvokeVoid=@JsMethode RedirectToLogin="/login" 
                 PublicRoutes="@PublicRoutes">
    <Router AppAssembly="typeof(Layout.SecondaryLayout).Assembly" NotFoundPage="typeof(Pages.Errors.NotFound)">
        <Found Context="routeData">
            <RouteView RouteData="routeData" DefaultLayout="typeof(Layout.SecondaryLayout)" />            
        </Found>
    </Router>
</AuthInitializer>

@code{
    public List<string> JsMethode = new List<string>()
    {
        "depensio.initialized"
    };

    public List<string> PublicRoutes = new List<string>()
    {
        "/register", "/verifier-mail", "/forgot-password", "/reset-password"
    };

}
